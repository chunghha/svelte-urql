{
  "name": "svelte-urql",
  "version": "0.1.0",
  "engines": {
    "node": ">=16.15.0"
  },
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"vite dev\" \"yarn codegen --watch\"",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --cache --write --plugin-search-dir=. \"**/*.{cjs, js, ts, svelte}\"",
    "lint": "eslint . --fix --ext .cjs,.js,.ts,.svelte",
    "test": "vitest",
    "test:browser": "playwright test",
    "coverage": "vitest run --coverage",
    "check:all": "yarn format && yarn lint",
    "codegen": "graphql-codegen"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^5.0.2",
    "@fontsource/lora": "^5.0.2",
    "@fontsource/nunito-sans": "^5.0.2",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/fragment-matcher": "^5.0.0",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typed-document-node": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-urql": "^3.7.3",
    "@graphql-codegen/typescript-urql-graphcache": "^2.4.5",
    "@graphql-codegen/urql-introspection": "^2.1.1",
    "@playwright-testing-library/test": "^4.5.0",
    "@playwright/test": "^1.35.0",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.20.2",
    "@sveltejs/vite-plugin-svelte": "^2.4.1",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "~5.59.9",
    "@typescript-eslint/parser": "~5.59.9",
    "@urql/exchange-graphcache": "^6.1.1",
    "@urql/svelte": "^4.0.2",
    "autoprefixer": "^10.4.14",
    "c8": "^7.14.0",
    "concurrently": "^8.2.0",
    "daisyui": "^3.0.22",
    "eslint": "~8.42.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte": "^2.30.0",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "jsdom": "^22.1.0",
    "playwright-testing-library": "^4.5.0",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.5.3",
    "typescript": "~5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.0"
  },
  "type": "module"
}
