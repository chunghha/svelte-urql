{
  "name": "svelte-urql",
  "version": "0.1.0",
  "engines": {
    "node": ">=16.15.0"
  },
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"vite dev\" \"yarn codegen --watch\"",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --cache --write --plugin-search-dir=. \"**/*.{cjs, js, ts, svelte}\"",
    "lint": "eslint . --fix --ext .cjs,.js,.ts,.svelte",
    "test": "vitest",
    "test:browser": "playwright test",
    "coverage": "vitest run --coverage",
    "check:all": "yarn format && yarn lint",
    "codegen": "graphql-codegen"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.9",
    "@fontsource/inter": "^4.5.12",
    "@fontsource/poppins": "^4.5.9",
    "@graphql-codegen/cli": "^2.6.4",
    "@graphql-codegen/fragment-matcher": "^3.2.1",
    "@graphql-codegen/near-operation-file-preset": "^2.2.14",
    "@graphql-codegen/typed-document-node": "^2.2.13",
    "@graphql-codegen/typescript": "^2.5.1",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@graphql-codegen/typescript-urql": "^3.5.12",
    "@graphql-codegen/typescript-urql-graphcache": "^2.2.16",
    "@graphql-codegen/urql-introspection": "^2.1.1",
    "@playwright-testing-library/test": "^4.3.1",
    "@playwright/test": "^1.25.2",
    "@sveltejs/adapter-auto": "^1.0.0-next.72",
    "@sveltejs/kit": "^1.0.0-next.480",
    "@sveltejs/vite-plugin-svelte": "^1.0.5",
    "@tailwindcss/typography": "^0.5.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.1",
    "@types/jest": "^29.0.1",
    "@typescript-eslint/eslint-plugin": "~5.36.2",
    "@typescript-eslint/parser": "~5.36.2",
    "@urql/exchange-graphcache": "^5.0.1",
    "@urql/svelte": "^3.0.1",
    "autoprefixer": "^10.4.8",
    "c8": "^7.12.0",
    "concurrently": "^7.2.1",
    "daisyui": "^2.27.0",
    "eslint": "~8.23.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "jsdom": "^20.0.0",
    "playwright-testing-library": "^4.3.1",
    "postcss": "^8.4.16",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.5.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.3.1",
    "typescript": "~4.8.3",
    "vitest": "^0.23.1",
    "vitest-svelte-kit": "^0.0.7"
  },
  "type": "module"
}
